# NextAuth Configuration
NEXTAUTH_URL=https://your-vercel-deployment-url.vercel.app
NEXTAUTH_SECRET=your-nextauth-secret-generated-with-openssl-rand-base64-32

# Google OAuth
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Google Sheets
GOOGLE_SPREADSHEET_ID=your-google-spreadsheet-id
GOOGLE_SHEETS_API_KEY=your-google-sheets-api-key

# Apps Script
APPS_SCRIPT_URL=https://script.google.com/macros/s/your-apps-script-deployment-id/exec

# Public Variables (accessible on client-side)
NEXT_PUBLIC_SPREADSHEET_ID=your-google-spreadsheet-id
